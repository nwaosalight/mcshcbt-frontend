    "graphql": "^15.8.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@prisma/client": "^4.16.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^18.19.99",
    "apollo-server-testing": "^2.20.0",
    "jest": "^29.7.0",
    "prisma": "^4.16.2",
    "ts-jest": "^29.3.2",
