{
  "name": "mcsh-backend",
  "version": "1.0.0",
  "description": "GraphQL Node.js TypeScript backend with Prisma",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
